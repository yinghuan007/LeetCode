class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        dict = {}
        for index, num in enumerate(nums):
            t_number = target - num
            if t_number in dict:
                return [dict[t_number],index]
            dict[num] = index
        return None
